@article{isikIntegrityAnalysisGPSBased2020,
  title = {Integrity {{Analysis}} for {{GPS-Based Navigation}} of {{UAVs}} in {{Urban Environment}}},
  author = {Isik, Oguz Kagan and Hong, Juhyeon and Petrunin, Ivan and Tsourdos, Antonios},
  year = {2020},
  month = sep,
  journal = {Robotics},
  volume = {9},
