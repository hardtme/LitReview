  author = {Li, Guohua and Eby, David W. and Santos, Robert and Mielenz, Thelma J. and Molnar, Lisa J. and Strogatz, David and Betz, Marian E. and DiGuiseppi, Carolyn and Ryan, Lindsay H. and Jones, Vanya and Pitts, Samantha I. and Hill, Linda L. and DiMaggio, Charles J. and LeBlanc, David and Andrews, Howard F.},
